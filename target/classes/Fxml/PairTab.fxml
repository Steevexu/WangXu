                <TextField fx:id="tNumberRelative" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Student 1" />
                <TextField fx:id="tStudent1" />
                <Text layoutX="21.0" layoutY="190.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Student 2" />
                <TextField fx:id="tStudent2" layoutX="21.0" layoutY="204.0" />
            </children>
        </VBox>
        <TableView fx:id="table3" layoutX="386.0" layoutY="110.0" onMouseClicked="#getData" prefHeight="260.0" prefWidth="473.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="120.0">
